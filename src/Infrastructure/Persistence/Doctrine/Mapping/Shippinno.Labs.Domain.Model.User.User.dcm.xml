<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">
    <entity name="Shippinno\Labs\Domain\Model\User\User"
            table="users"
            repository-class="Shippinno\Labs\Infrastructure\Domain\Model\User\DoctrineUserRepository">
        <id name="userId" column="id" type="user_id"/>
        <field name="nickname" column="nickname"/>
    </entity>
</doctrine-mapping>